<%# binding.pry %>

<%= simple_form_for([@project, @lesson],
                    wrapper_mappings: { boolean: :vertical_boolean }, input_html: {multipart: true}) do |f| %>

  <%= f.input :name %>
  <%= f.input :video, as: :file %>
  <%= f.input :description %>
  <%= f.input :pdf %>
  <%= f.input :logo %>

  <%= f.button :submit, class: 'btn-primary' %>
<% end %>